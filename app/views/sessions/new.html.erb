<%= form_for(:session, :url => sessions_path) do |f| %>
  <%= f.label :email %><br />
  <%= f.text_field :email %>
  
  <%= f.label :password %><br />
  <%= f.password_field :password %>
  
  <%= f.submit 'Sign in' %>
<% end %>
<% flash.each do |key, value| %>
  <p><%= key %>: <%= value %></p>
<% end %>